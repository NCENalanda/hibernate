<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate?Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nt.domain.ProgrammerProjectInfo" table="PROGRAMMERS_PROJECTS_INFO">
    <!-- singular Id Field-->
     <composite-id>
       <key-property name="prgmrId" column="PRGMRID"/>
       <key-property name="projId" column="PROJID"/>
     </composite-id>
    <property name="prgmrName" length="15"/>
    <property name="projName" length="15"/>
    <property name="status" length="10"/>
  </class>

</hibernate-mapping>  