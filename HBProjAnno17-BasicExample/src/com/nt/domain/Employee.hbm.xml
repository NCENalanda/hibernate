<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate?Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.nt.domain.EmpDetails" table="Employee">
    <id name="no"  column="EID"  type="int" >
      <generator class="increment"/>
    </id>
    <property name="fname" column="FIRSTNAME" type="string" length="15"/>
    <property name="lname" column="LASTNAME"  type="string" length="15"/>
    <property name="mail" column="EMAIL" type="string"  length="20" insert="false"/>
  </class>
</hibernate-mapping>
